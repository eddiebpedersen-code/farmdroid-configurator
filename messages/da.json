  "wheelConfig": {
    "title": "Hjulkonfiguration",
    "subtitle": "Vælg mellem 3-hjuls og 4-hjuls opsætning",
    "learnMore": "Lær mere om hjulkonfigurationer",
    "moreInfo": "Mere information",
    "options": {
      "pfw": {
        "name": "Passivt Forhjul",
        "subtitle": "Bedst til flade marker med bredere rækkeafstand (45-50 cm)",
        "description": "Standard hjulkonfigurationen giver stabilitet og jævn styring på fladt terræn. Ideel til afgrøder med lige antal rækker og bredere rækkeafstand, såsom sukkerroer (45-50 cm)."
      },
      "afw": {
        "name": "Aktivt Forhjul",
        "subtitle": "Auto-vippende stabilitet for skråninger over 8%",
        "description": "Til kuperet terræn tilføjer det aktive forhjul en aktuator, der automatisk vipper forhjulet for at opretholde stabiliteten. Anbefales til skråninger over 8% og sidehældninger over 5%, og til afgrøder med lige antal rækker og bredere afstand (omkring 45-50 cm)."
      },
      "dfw": {
        "name": "Dobbelt Forhjul",
        "subtitle": "Smal rækkeafstand ned til 22,5 cm og bedsystemer",
        "description": "Udstyret med to forhjul i stedet for ét, forbedrer denne opsætning balance og præcision, særligt for afgrøder med smal rækkeafstand ned til 22,5 cm. Perfekt til flade marker og bedsystemer, fungerer godt med både lige og ulige antal rækker."
      }
    },
    "configTypes": {
      "threeWheel": "3-hjul",
      "fourWheel": "4-hjul",
      "openField": "Åben mark",
      "bedConfig": "Bedkonfiguration"
